   26:	  0044	F5 B0		PISCA:			MOV	P3,A				;escreve no port 3
   27:	  0046	12 00 66				LCALL	ATRASO				;chama a rotina de atraso
   28:	  0049	C4					SWAP	A				;troca os 4bits +sig, pelos 4 -sig
   29:	  004A	A8 90					MOV	R0,P1				;lê teclado
