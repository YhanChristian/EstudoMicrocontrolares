    8:				; INSTRUÇÕES LÓGICAS
    9:
   10:					  ; Este programa tem como função exemplificar a criação de máscaras com a utilização de
   11:				          ; funções lógicas as quais podem ser utilizadas para alterar Bits específicos de registrad
				ores sem
   12:				          ; a alteração de todo seu conteúdo. Neste exemplo criamos uma máscara com a função OR
   13:				          ; para setarmos apenas o Bit 1 do registrador R0.
   14:
   15:		N      0000		ORG 0000H           		; Endereço de início de programa.
   16:
   17:	  0000	78 91		     	MOV R0, # 10010001B   		; Coloca em R0 o referido valor binário.
   18:	  0002	74 02		     	MOV A,   # 00000010B    	; Coloca o referido valor binário em ACC.
   19:	  0004	48		     	ORL A, R0            		; Realiza o OU lógico (mascaramento) entre A e R0.
   20:
   21:	  0005	F8			MOV R0, A            		; Setamos apenas o segundo BIT do valor presente
   22:									; em R0 o resultado fica em ACC e então colocamos
   23:									; o resultado em R0.
   24:
   25:	  0006	80 FE		     	SJMP $              		; Trava o programa nesta linha.
   26:
   27:				     	END                 		; Fim de programa.
