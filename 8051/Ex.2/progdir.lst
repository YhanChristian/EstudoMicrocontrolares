    6:
    7:				; Este programa não tem função prática, seu objetivo é apresentar as principais DIRETIVAS DE COMPILA
				ÇÃO ; da maioria dos compiladores Assembly
    8:
    9:				; São elas:  EQU - Iguala para posterior substituição
   10:				;                  ORG - Indicativo de endereço da memória de programa
   11:				;                  DB  - Aloca dados na memória de programa seqüencialmente
   12:				;                  END - Fim de compilação
   13:
   14:				; Definições antes do programa
   15:		N      0008	              R8 EQU 08H           	; Det. que chamarei de R8 o endereço 08H (RAM Interna)
   16:		N      0009	              R9 EQU 09H           	; Det. que chamarei de R9 o endereço 09H (RAM Interna)
   17:
   18:				; Programa
   19:		N      0000	              ORG 0000H            	; Endereço que indica início do meu programa na ROM
   20:
   21:	  0000	78 41		              MOV R0, #'A'   	        ; Colocar em R0 o códico ASCII da letra A
   22:	  0002	75 08 05	              MOV R8, #05H       	; Colocar em R8(08H) o valor 05H
   23:	  0005	74 FE		              MOV A, #0FEH  	        ; Colocar em ACC o valor F0H
   24:	  0007	F5 09		              MOV R9, A             	; Colocar em R9(09H) o valor de ACC
   25:
   26:				                                        ; Endereço de início para a alocação dos caracteres
   27:		N      0010	              ORG 0010H            	; Endereço da ROM onde gravaremos os dados a seguir
   28:
   29:	  0010	53 49 4C 56	              DB 'SILVIO'     	        ; A partir de 10H da ROM escreveremos cada uma das letras em
	  0014	49 4F		 ASCII
   30:				              END                  	; Diretiva que indica fim de programa para o compilador.





