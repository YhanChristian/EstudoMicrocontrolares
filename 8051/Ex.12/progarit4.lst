    7:				; INSTRUÇÕES ARITMÉTICAS
    8:
    9:				; Programa para treino de instruções matemáticas, nele é realizado a subtração dos valores
   10:				 ; contidos nos registradores R0 e R1, lembrando que sempre devemos utilizar o Acc para a
   11:				   ; realização desse tipo de instrução e o resultado também ficará no Acc, quando simular esse
   12:				   ; programa altere os valores de R0 e R1 e  observe o BIT "C" (Carry), que é o 7º Bit do PSW.
   13:
   14:				;Ordem: Acc - (valor).
   15:				;Acc -> fica o resultado.
   16:
   17:		N      0000	          		ORG 0000H  	; End. de início de programa
   18:
   19:	  0000	78 05		          		MOV R0, # 05  	; Coloco valor em R0
   20:	  0002	79 0A		          		MOV R1, # 10  	; Coloco valor em R1
   21:
   22:	  0004	E9		          		MOV A, R1    	; Copio R1 para Acc p/ realizar a subtração
   23:	  0005	98		          		SUBB A, R0   	; Realiza Acc - R0
   24:
   25:	  0006	FF		          		MOV R7, A    	; Guardo o resultado em R7
   26:
   27:	  0007	80 FE		          		SJMP $      	; Trava o programa nesta linha
   28:
   29:				          		END         	; Fim de compilação





