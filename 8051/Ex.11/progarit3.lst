    7:				; INSTRUÇÕES ARITMÉTICAS
    8:
    9:				; Exemplo de programa para utilização de instruções matemáticas, nele multiplicaremos os valores de
				8
   10:				  ; Bits contidos nos registradores R0 e R1e guardaremos o resultado de 16 Bits nos registradores R3
				 (MSB)
   11:				  ; e R4 (LSB) lembrando que a instrução de multiplicação MUL só é realizada entre os registradores.
   12:				; A e B .
   13:
   14:				;-> B fica o Byte MSB do resultado.
   15:				;-> A fica o Byte LSB do resultado.
   16:
   17:		N      0000	     		ORG 0000H    		; Endereço de início do programa.
   18:
   19:	  0000	78 64		     		MOV R0, # 100  		; Colocar em R0 o valor 100.
   20:	  0002	79 0B		     		MOV R1, # 11   		; Colocar em R1 o valor 11.
   21:
   22:	  0004	E8		     		MOV A, R0     		; Colocar em ACC o valor de R1.
   23:	  0005	89 F0		     		MOV B, R1     		; Colocar em B o valor de R2.
   24:	  0007	A4		    		MUL AB       		; Multiplicar os conteúdos de ACC e B.
   25:
   26:	  0008	AB F0		     		MOV R3, B     		; Copiando o MSB da multiplicação para R3.
   27:	  000A	FC		     		MOV R4, A     		; Copiando o LSB da multiplicação para R4.
   28:
   29:	  000B	80 FE				SJMP $       		; Trava o programa nesta linha (loop infinito).
   30:
   31:				     		END          			; Fim de programa.
